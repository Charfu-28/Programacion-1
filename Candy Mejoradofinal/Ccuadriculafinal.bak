;Sumador
(define (lento posx posy posimg vec j pos)
  (if (<= j 8000)    
       (lento posx posy posimg vec (+ j (/ 1 300)) pos)
      (begin
        ((draw-solid-rectangle w) (make-posn posx posy) 80 80 "black")
        ((draw-solid-rectangle w) posimg  80 80 "black")
        (vector-set! vec pos 1)
        (hacer-click (mouse-click-posn (get-mouse-click w)) vec)
                 )
    )
 )
          
;Sumador 2
(define (lento2 posimg posx posy vec j)
  (if (<= j 8000)
      
       
      (lento2 posimg posx posy vec (+ j (/ 1 300)))
      
      (begin
        ((draw-solid-rectangle w) (make-posn posx posy) 80 80 "yellow")
        ((draw-solid-rectangle w) posimg 80 80 "yellow")
      
      
      (hacer-click (mouse-click-posn (get-mouse-click w)) vec)
                 )
    )
 )
  
