(define (funcion n)
  (cond
    [(and (integer? n) (>= n 0)) (cond
                                   [(= n 0) 0]
                                   [(and (<= n 400) (>= n 1)) 1]
                                   [else (+ (floor (/ n 300)) 1)])]
    [display "Error"]))
(define (timer n i)
  (cond
    [(= n 0) (display (funcion n))]
    [else (cond
            [(< i n) (begin ((draw-solid-rectangle ventana)(make-posn 10 470) (funcion i) 20 "black")  (timer n (+ i 1)))]
            [else (begin (close-viewport ventana) )
                 ])]))
(define (Time n)
  (if (= n 0)
      (timer 0 0)
  (begin
    (iniciosonido #t)
  (timer n 0)
  )
  ))
 

