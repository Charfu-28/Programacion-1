;Sumador
(define (lento posx posy posimg vec j pos l)
  (if (<= j 7000)
      (lento posx posy posimg vec (+ j (/ 1 300)) pos l)
       (begin
         (s #t) 
        ((draw-solid-rectangle w) (make-posn posx posy) 80 80 "black")
        ((draw-solid-rectangle w) posimg  80 80 "black")
        (vector-set! vec pos 1)
        (if (= l 50) 
        (if (equal? #t (vectorlleno vec 0))
          (begin
          (clear-viewport w)
          (((draw-pixmap-posn "YouWin.png") w)(make-posn 0 0) "red")
          (Winner #t)
          (final (mouse-click-posn (get-mouse-click w)))
          
          )
