(define (lineas fila columna)
  (if (or  (<= columna C) (<= fila F))
     (if (<= columna C)          
            ((draw-solid-rectangle w) (make-posn (* 82 columna) 0) 2 657 "black")
            (lineas fila (+ columna 1))
          )
          (if (<= fila F)
            ((draw-solid-rectangle w) (make-posn 0 (* 82 fila)) 657 2 "black")
            (lineas (+ fila 1) columna)
          )
      )
  )


;Tiempo
(define (tiempo posx posy posimg vec)
  (if (>= (- (current-milliseconds) tiempo_inicial) 3000)
      (begin
      ((draw-solid-rectangle w) (make-posn posx posy)  "black")
      ((draw-solid-rectangle w) posimg  "black")
      (hacer-click (mouse-click-posn (get-mouse-click w)) (vector-set! vec 36 1))
                 )
      (begin
        (void)
        (tiempo posx posy posimg vec)
      )
  )
)
  

;Tiempo
(define (tiempo2  rect vec)
  (if (>= (- (current-milliseconds) tiempo_inicial) 3000)
      (begin
      rect
      (hacer-click (mouse-click-posn (get-mouse-click w)) vec)
      )
      (begin
        (void)
        (tiempo2 rect vec)
      )
  )
)


;--------------------------------------------
(define (revisar clic posx posy tamxy vec)
   (if (and (= posx 2) (= posy 2))
       (if (and (= 3 (floor (/ (* (posn-y clic) 8) alto))) (= 4 (floor (/ (* (posn-x clic) 8) ancho))))
          (begin
            ((clear-solid-rectangle w) posimg7 80 80 )
            img7
            (tiempo posx posy posimg7 vec)
          )
          (begin
            (dibujar (floor (/ (* (posn-x clic) 8) ancho)) (floor (/ (* (posn-y clic) 8) alto)))
            (tiempo2  A1 (posiciones (floor (/ (* (posn-x clic) 8) ancho)) (floor (/ (* (posn-y clic) 8) alto))) vec)
          )      
      )
    (close-viewport w)
  )
)
